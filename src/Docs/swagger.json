{
    "openapi": "3.0.3",
    "info": {
        "title": "transactions-api",
        "description": "API Rest",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "email": "victorbentesdev@gmail.com"
        },
        "license": {
            "name": "",
            "url": ""
        },
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://localhost:3080",
            "description": "API DE DEV"
        }
    ],
    "tags": [
        {
            "name": "Users",
            "description": "Operations about user"
        },
        {
            "name": "Stores",
            "description": "Access to Auth orders"
        },
        {
            "name": "Transactions",
            "description": "Operations about documents"
        }
    ],
    "paths": {
        "/user/create": {
            "post": {
                "tags": [
                    "Users"
                ],
                "summary": "Add a new user",
                "requestBody": {
                    "description": "Add a new user",
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/User"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/User"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Bad Request"
                    },
                    "404": {
                        "description": "Page not found"
                    },
                    "405": {
                        "description": "Validation exception"
                    }
                }
            }
        },
        "/auth/login": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Login",
                "description": "Login Users",
                "requestBody": {
                    "description": "Login Request",
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/Login"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "$ref": "#/components/schemas/LoginResponse"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Bad Request"
                    },
                    "404": {
                        "description": "Page not found"
                    },
                    "405": {
                        "description": "Validation exception"
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "Login": {
                "type": "object",
                "properties": {
                    "email": {
                        "type": "string"
                    },
                    "password": {
                        "type": "string"
                    }
                }
            },
            "ApiResponse": {
                "type": "object",
                "properties": {
                    "code": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "type": {
                        "type": "string"
                    },
                    "message": {
                        "type": "string"
                    }
                },
                "xml": {
                    "name": "##default"
                }
            }
        }
    }
}